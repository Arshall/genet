{
  "name": "@genet/base",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Basic definitions",
  "engines": {
    "genet": "*"
  },
  "homepage": "https://genet.app/",
  "genet": {
    "components": [
      {
        "type": "core:token",
        "main": "tokens.json"
      },
      {
        "type": "core:renderer:attr",
        "id": "@flags",
        "main": "flags.js"
      },
      {
        "type": "core:renderer:attr",
        "id": "@enum",
        "main": "enum.js"
      },
      {
        "type": "core:renderer:attr",
        "id": "@nested",
        "main": "nested.js"
      },
      {
        "type": "core:renderer:attr",
        "id": "@novalue",
        "main": "novalue.js"
      },
      {
        "type": "core:filter:macro",
        "id": "unixdate",
        "name": "UNIX Date",
        "description": "{{prefix}}2014-09-08T08:02:17-05:00",
        "main": "unixdate-macro.js"
      },
      {
        "type": "core:renderer:attr",
        "id": "@date:unix",
        "main": "unixdate.js",
        "macro": "unixdate-macro-rev.js"
      }
    ]
  }
}