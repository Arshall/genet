{
  "name": "@genet/ipv6",
  "version": "0.1.0",
  "license": "MIT",
  "description": "IPv6 dissector",
  "engines": {
    "genet": "*"
  },
  "genet": {
    "components": [
      {
        "type": "core:library",
        "main": "ipv6"
      },
      {
        "type": "core:filter:macro",
        "id": "ipv6-literal",
        "name": "IPv6 Address",
        "description": "{{prefix}}::1",
        "main": "macro.js"
      },
      {
        "type": "core:renderer:attr",
        "id": "@ipv6:addr",
        "main": "ipv6-addr.js",
        "macro": "macro-rev.js"
      },
      {
        "type": "core:style",
        "main": "ipv6.css"
      },
      {
        "type": "core:token",
        "main": "tokens.json"
      }
    ]
  }
}