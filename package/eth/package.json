{
  "name": "@genet/eth",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Ethernet dissector",
  "engines": {
    "genet": "*"
  },
  "genet": {
    "components": [
      {
        "type": "core:library",
        "main": "eth"
      },
      {
        "type": "core:renderer:attr",
        "id": "@eth:mac",
        "main": "mac-addr.js",
        "macro": "macro-rev.js"
      },
      {
        "type": "core:filter:macro",
        "name": "MAC Address",
        "description": "{{prefix}}00:00:00:00:00:00",
        "main": "macro.js"
      },
      {
        "type": "core:style",
        "main": "eth.css"
      },
      {
        "type": "core:token",
        "files": [
          "tokens.json"
        ]
      }
    ]
  }
}